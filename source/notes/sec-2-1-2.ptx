                <exercise>
                    <title>Problems</title>
					
					<task workspace="2in">
						<statement>
							<p>
								Let <m>\{A_{n}\}_{n=1}^{26}</m> be the collection discussed previously.
								Find
								<ol>
								<li>
									<p>
									<m>\displaystyle \bigcup_{n=1}^{26}A_{n}</m>
									</p>
								</li>

								<li>
									<p>
									<m>\displaystyle \bigcap_{n=1}^{26}A_{n}</m>
									</p>
								</li>
								</ol>
							</p>
						</statement>
					</task>

					<task workspace="2in">
						<statement>
							<p>
								For each <m>r \in \mathbb{Q}</m>, let <m>N_{r} = \mathbb{R}\setminus \{r\}</m>.
								Find
								<ol>
								<li>
									<p>
									<m>\displaystyle \bigcup_{r \in \mathbb{Q}}N_{r}</m>
									</p>
								</li>

								<li>
									<p>
									<m>\displaystyle \bigcap_{r \in \mathbb{Q}}N_{r}</m>
									</p>
								</li>
								</ol>
							</p>
						</statement>
					</task>

					<task workspace="2in">
						<statement>
							<p>
								Draw a Venn diagram of a collection of three sets that are pairwise disjoint.
							</p>
						</statement>
					</task>

					<task workspace="2in">
						<statement>
							<p>
								Provide an example of a collection of three sets <m>\{A_{1}, A_{2}, A_{3}\}</m> that are <em>not</em> pairwise disjoint, but <m>\displaystyle \bigcap_{n=1}^{3} A_{n} = \emptyset</m>.
							</p>
						</statement>
					</task>

					<task workspace="2in">
						<statement>
							<p>
								Find a collection of nonempty sets <m>S_{i} \subseteq \mathbb{N}</m> indexed by <m>i \in \mathbb{N}</m> such that <m>S_{i+1}\subsetneq S_{i}</m> and <m>\displaystyle \bigcap_{i=1}^{\infty}S_{i} = \emptyset</m>.
							</p>
						</statement>
					</task>

					<task workspace="2in">
						<statement>
							<p>
								Find a collection of nonempty sets <m>S_{i} \subseteq \mathbb{N}</m> indexed by <m>i \in \mathbb{N}</m> such that <m>S_{i}\subsetneq S_{i+1}</m> and <m>\displaystyle \bigcup_{i=1}^{\infty}S_{i} \neq \mathbb{N}</m>.
							</p>
						</statement>
					</task>

					<task workspace="4in">
						<statement>
							<p>
								Let <m>\{A_{\alpha}\}_{\alpha \in \Delta}</m> be a collection of sets.
								Prove <em>one</em> of the following.
								<ol>
								<li>
									<p>
									<m>\left[ \displaystyle \bigcup_{\alpha \in \Delta}A_{\alpha}\right]^{C} = \displaystyle \bigcap_{\alpha \in \Delta}A_{\alpha}^{C}</m>
									</p>
								</li>

								<li>
									<p>
									<m>\left[ \displaystyle \bigcap_{\alpha \in \Delta}A_{\alpha}\right]^{C} = \displaystyle \bigcup_{\alpha \in \Delta}A_{\alpha}^{C}</m>
									</p>
								</li>
								</ol>
							</p>
						</statement>
					</task>

					<task workspace="4in">
						<statement>
							<p>
								Let <m>\{A_{\alpha}\}_{\alpha \in \Delta}</m> be a collection of sets and let <m>B</m> be any set.
								Prove <em>one</em> of the following.
								<ol>
								<li>
									<p>
									<m>B \cup \left[ \displaystyle \bigcap_{\alpha \in \Delta}A_{\alpha}\right] = \displaystyle \bigcap_{\alpha \in \Delta}(B\cup A_{\alpha})</m>
									</p>
								</li>

								<li>
									<p>
									<m>B \cap \left[ \displaystyle \bigcup_{\alpha \in \Delta}A_{\alpha}\right] = \displaystyle \bigcup_{\alpha \in \Delta}(B \cap A_{\alpha})</m>
									</p>
								</li>
								</ol>
							</p>
						</statement>
					</task>
                
                    
                </exercise>
   
        

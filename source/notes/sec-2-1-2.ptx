                <exercise>
                    <title>Problems</title>
					<task workspace="1in">
						<statement>
							<p>
								If <m>A</m> is a set, what is <m>A \times \emptyset</m> equivalent to?
							</p>
						</statement>
					</task>

					<task workspace="1in">
						<statement>
							<p>
							Given sets <m>A</m> and <m>B</m>, when does <m>A \times B = B \times A</m>?
							</p>
						</statement>
					</task>
            
